{
  "name": "JC-VAD",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "scripts": {
    "cp": "node .github/cp.js",
    "rm": "node .github/rm.js",
    "mkdir": "node .github/mkdir.js",
    "ts:build": "tsc && yarn cp package.json ./dist/",
    "c:release": "cmake-js compile && yarn mkdir dist && yarn cp ./build/Release/vad.node /dist/",
    "electron": "electron-build-env yarn c:release",
    "release": "yarn rm ./dist && yarn mkdir ./dist && yarn electron && yarn ts:build && yarn cp ./lib/*.d.ts ./dist",
    "test": "yarn release && node dist/test.js"
  },
  "dependencies": {
    "electron": "^9.1.1",
    "electron-build-env": "^0.2.0",
    "node-addon-api": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "cmake-js": "^6.2.1"
  }
}
